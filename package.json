  "description": "",
  "main": "index.js",
  "preferGlobal": true,
  "bin": "./commands.js",
  "dependencies": {
    "commander": "^2.15.1",
    "inquirer": "^5.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  },
  "scripts": {
    "global": "npm link",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "./node_modules"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
