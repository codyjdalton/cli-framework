{
  "name": "jule",
  "version": "0.1.0",
  "description": "",
  "main": "/lib/program.js",
  "preferGlobal": true,
  "bin": "./lib/program.js",
  "dependencies": {
    "commander": "^2.15.1",
    "inquirer": "^5.2.0"
  },
  "devDependencies": {
    "chai": "*",
    "coveralls": "*",
    "mocha": "*",
    "nyc": "*"
  },
  "scripts": {
    "global": "npm link",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/**/*.spec.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyjdalton/jule.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
